{
    "liveServer.settings.port": 5502,
    "editor.tokenColorCustomizations": {
        "textMateRules": [
            {
                "scope": "keyword.control.bg3txt",
                "settings": {
                    "foreground": "#C586C0"
                }
            },
            {
                "scope": "keyword.operator.bg3txt",
                "settings": {
                    "foreground": "#ffa200"
                }
            },
            {
                "scope": "string.first-column.bg3txt",
                "settings": {
                    "foreground": "#9CDCFE"
                }
            }
        ]
    },
    "files.associations": {
        "*.vue": "vue-html",
        "*.txt": "bg3txt"
    },
    "githubPullRequests.ignoredPullRequestBranches": [
        "main"
    ]
}